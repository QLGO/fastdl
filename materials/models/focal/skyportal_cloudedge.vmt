"UnlitGeneric"
{
	"$basetexture" "focal/cloud_swirl4"
	"$additive" 1
	"$alpha" 0.5
	
	"$center"		"[0.5 0.5]"
	"$rotate"       "0"
	"$translate"    "[0 0]"
	
	Proxies
	{		
		LinearRamp 
		{
			rate -15
			resultVar	"$rotate"
		}
		
		
		TextureTransform
		{	
			centerVar		"$center"
			rotateVar		"$rotate"
			translateVar	"$translate"
			resultVar		"$basetexturetransform"
		}
		
	}
}
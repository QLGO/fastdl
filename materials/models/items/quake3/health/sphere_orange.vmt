"UnlitGeneric"
{
	"$basetexture" "models/items/quake3/envmapgold2_b"
	//"$color" "[.65 .65 .65]"
	"$additive" 1
	
	"$angle" 0.0
	"$center" "[3 6]"
	"$scale" "[1.5 1.5]"

	
	"Proxies"
	{
		"LinearRamp"
		{
			"rate" 10
			"initialValue" 0.0
			"resultVar" "$angle"
		}


		"TextureTransform"
		{
			"centerVar" "$center"
			"rotateVar" "$angle"
			"scaleVar" "$scale"
			"resultVar" "$basetexturetransform"
		}
   }
}
"VertexlitGeneric"
{
	"$basetexture" "phantgothic/phantgothic_jp_orange_fx"

	"$angle" 0.0
	"$translate" "[0.0 0.0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"LinearRamp"
		{
			"rate" -75
			"initialValue" 0.0
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}

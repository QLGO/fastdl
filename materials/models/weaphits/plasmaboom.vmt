"UnlitGeneric"
{
	"$basetexture" "models/weaphits/plasmaboom"
	"$additive" 1
	"$nocull" 1
	
	"$angle" 0.0
	"$sc" 0.0
	"$center" "[.5 .5]"
	"$scale" "[1 1]"
	
	"Proxies"
	{
		"LinearRamp"
		{
			"rate" -999
			"initialValue" 0.0
			"resultVar" "$angle"
		}
		
		"Sine"
		{
			"sineperiod" 0.125
			"sinemin"	0.85
			"sinemax"	1.1
			"resultVar"	"$sc"
		}
		
		"Equals"
		{
			"srcVar1"	"$sc"
			"resultVar"	"$scale[0]"
		}
		
		"Equals"
		{
			"srcVar1"	"$sc"
			"resultVar"	"$scale[1]"
		}


		"TextureTransform"
		{
			"centerVar" "$center"
			"rotateVar" "$angle"
			"scaleVar" "$scale"
			"resultVar" "$basetexturetransform"
		}
	
   }
}
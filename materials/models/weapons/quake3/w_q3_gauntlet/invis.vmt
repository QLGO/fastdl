"UnlitGeneric"
{
	"$basetexture" "effects/invismap"
	"$additive" 1
	$color "[3 3 3]"
	
	"$angle" 0.0
	"$center" "[-2 1]"
	"$scale" "[0.5 0.5]"

	
	"Proxies"
	{		
		"Sine"
		{
			"sineperiod" 5
			"sinemin" -10.0
			"sinemax" 10.0
			"resultVar" "$angle"
		}


		"TextureTransform"
		{
			"centerVar" "$center"
			"rotateVar" "$angle"
			"scaleVar" "$scale"
			"resultVar" "$basetexturetransform"
		}
   }
}
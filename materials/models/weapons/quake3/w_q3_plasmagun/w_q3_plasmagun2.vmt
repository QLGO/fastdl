"UnlitGeneric"
{
	"$baseTexture" "models/weapons/quake3/w_q3_plasmagun/w_q3_plasmagun2"
	"$envmap" "models/items/quake3/tinfxb"
	"$envmaptint" "[.1 .1 .1]"

	"$angle" 0.0
	"$translate" "[0.0 0.0]"
	"$center" "[0.9 0.9]"

	"Proxies"
	{
		"LinearRamp"
		{
			"rate" -100
			"initialValue" 0.0
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}

